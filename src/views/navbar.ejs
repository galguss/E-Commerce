<nav>
        <li><a href="/cart"><img src="/cart.png" alt="cart-icon"></a></li>
        <li><a href="/">Home</a></li>
        <% if(!token) {%>
        <li><a href="/user/sign-up">Sign Up</a></li>  
        <li><a href="/user/sign-in">Sign In</a></li>
        <% } else { %>
        <% if(user && user.role !== 'ADMIN'){ %>
